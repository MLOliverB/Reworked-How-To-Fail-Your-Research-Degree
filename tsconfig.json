{
    "compilerOptions": {
        "outDir": "bin",

        "lib": ["ES6", "dom"],
        "module": "commonjs",
        "target": "ES6",
    
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
      },
    "include": [
        "src/**/*",
        "test/**/*",
        "server.ts",
    ]
}